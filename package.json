{
  "name": "track-app",
  "version": "1.0.4",
  "description": "Time tracking desktop app for developers with Notion integration",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"pnpm run dev:renderer\" \"pnpm run dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "npx tsc -p tsconfig.main.json && electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "npx tsc -p tsconfig.main.json",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:mac:unsigned": "npm run build && electron-builder --mac --publish never",
    "dist:mac:dir": "npm run build && electron-builder --mac --dir --publish never",
    "test:prod": "npm run build && electron .",
    "test:prod:mac": "npm run build && electron . --mac",
    "test:quick": "./quick-test.sh",
    "test:full": "./test-prod.sh",
    "test:debug": "./debug-prod.sh",
    "status": "./check-status.sh",
    "build:optimized": "./optimize-build.sh",
    "clean": "rm -rf dist release node_modules/.cache",
    "clean:all": "rm -rf dist release node_modules pnpm-lock.yaml && pnpm install",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "electron",
    "time-tracking",
    "productivity",
    "notion",
    "macos"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@shadcn/ui": "^0.0.4",
    "@types/node": "^20.19.9",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron": "^27.1.3",
    "electron-builder": "^24.6.4",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "prebuild-install": "^7.1.3",
    "tailwindcss": "3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "conf": "^10.2.0",
    "dot-prop": "^6.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "electron-store": "^8.2.0",
    "form-data": "^4.0.4",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.536.0",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^9.0.1",
    "webidl-conversions": "^3.0.1"
  },
  "build": {
    "appId": "com.trackapp.dev",
    "productName": "Track App",
    "electronVersion": "27.3.11",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**",
      "assets/**",
      "package.json"
    ],
    "asar": true,
    "asarUnpack": [],
    "extraResources": [],
    "includeSubNodeModules": true,
    "compression": "maximum",
    "removePackageScripts": true,
    "removePackageKeywords": true,
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icon.icns",
      "identity": null,
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "dmg": {
      "sign": false
    }
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
